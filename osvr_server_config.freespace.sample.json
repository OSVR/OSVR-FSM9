{
  "plugins": [
    "com_osvr_Freespace"
  ],
  "routes": [
    {
      "destination": "/me/head",
      "source": {
	  "child": {
          "changeBasis": {
            "x": "-y",
            "y": "-z",
            "z": "-x"
          },
          "child": {
            "rotate": {
              "degrees": 0,
              "axis": "x"
            },
            "child": {
				"tracker": "/com_osvr_Freespace/Freespace",
				"sensor": 0
			}
          }
        }       
      }
    }
  ]
}
